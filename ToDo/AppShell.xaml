<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ToDo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ToDo"
    Title="ToDo">
    
    <FlyoutItem Title="Pages">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}"/>
    </FlyoutItem>
    
    <FlyoutItem Title="Tests">
        <Tab Title="Test">
            <ShellContent ContentTemplate="{DataTemplate local:TestPage}"/>
        </Tab>
        <Tab Title="AnotherTest">
            <ShellContent ContentTemplate="{DataTemplate local:AnotherTest}"/>
        </Tab>
    </FlyoutItem>
    
</Shell>
